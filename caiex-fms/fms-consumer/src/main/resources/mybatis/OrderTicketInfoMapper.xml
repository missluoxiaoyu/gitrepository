<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.caiex.account.mapper.OrderTicketInfoMapper">

<!-- order_ticket_info table all fields -->
<sql id="Base_Column_List" >
	 inplay,tkId,sellingTime,unitInvestment,totalInvestment,agentId,uid,bonus,bettingProducts,local_m,trade_price,total_price,tid,ballType,id
</sql>

<select id="getOrderTicketInfoBytid" parameterType="java.lang.String" resultMap="OrderTicketInfoResult">
	select * from order_ticket_info where tid=#{tid}
</select>
 

 
 <!-- 渠道 -->
 <select id="queryBatch" resultMap="OrderTicketInfoResult" parameterType="java.util.List">
	select * from  order_ticket_info 
	where id in 
    <foreach item="item" index="index" collection="list" open="(" separator="," close=")">
            #{item.ticketInfoId}
    </foreach>
    union all
    select * from  order_ticket_info_his 
	where id in 
    <foreach item="item" index="index" collection="list" open="(" separator="," close=")">
            #{item.ticketInfoId}
    </foreach>
</select>



 
 
 <resultMap type="com.caiex.account.entity.OrderTicketInfo" id="OrderTicketInfoResult">
 	<id column="id" property="id"/>
 	<result column="inplay" property="inplay"/>
 	<result column="tkId" property="tkId"/>
 	<result column="sellingTime" property="sellingTime"/>
 	<result column="unitInvestment" property="unitInvestment"/>
 	<result column="totalInvestment" property="totalInvestment"/>
 	<result column="agentId" property="agentId"/>
 	<result column="uid" property="uid"/>
 	<result column="bonus" property="bonus"/>
 	<result column="bettingProducts" property="bettingProducts"/>
 	<result column="local_m" property="localm"/>
 	<result column="trade_price" property="tradePrice"/>
 	<result column="total_price" property="totalPrice"/>
 	<result column="tid" property="tid"/>
 	<result column="ballType" property="ballType"/>
 </resultMap>
 
</mapper>   
