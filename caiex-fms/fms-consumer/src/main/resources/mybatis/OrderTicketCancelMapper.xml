<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.caiex.account.mapper.OrderTicketCancelMapper" > 
<!-- Result Map-->

<resultMap id="BaseResultMap" type="com.caiex.account.entity.OrderTicketCancel" >
	<result column="order_id" property="order_id"/>
	<result column="ticketInfo_id" property="ticketInfoId"/>
	<result column="agent_id" property="agent_id"/>
	<result column="trade_time" property="trade_time"/>
	<result column="trade_price" property="trade_price"/>
	<result column="trade_type" property="trade_type"/>
	<result column="state" property="state"/>
	<result column="recycleState" property="recycleState"/>
	<result column="recyclePrice" property="recyclePrice"/>
	<result column="winMoney" property="winMoney"/>
</resultMap>
       
<!-- order_ticket_cancel table all fields -->
<sql id="Base_Column_List" >
	 order_id,ticketInfo_id,agent_id,trade_time,trade_price,trade_type,state,recycleState,recyclePrice,winMoney
</sql>
   
   
<!-- 查询条件 -->
<sql id="Example_Where_Clause">
where 1=1
<trim  suffixOverrides="," >
	<if test="order_id != null" >
	    and order_id =  #{order_id}
	</if>
	<if test="tid != null  and tid!=''" >
	    and ticketInfo_id =  #{tid}
	</if>
	<if test="agent_id != null" >
	    and agent_id =  #{agent_id}
	</if>
	<if test="trade_time != null" >
	    and trade_time =  #{trade_time}
	</if>
	<if test="trade_price != null" >
	    and trade_price =  #{trade_price}
	</if>
	<if test="trade_type != null" >
	    and trade_type =  #{trade_type}
	</if>
	<if test="state != null" >
	    and state =  #{state}
	</if>
	<if test="recycleState != null" >
	    and recycleState =  #{recycleState}
	</if>
	<if test="recyclePrice != null" >
	    and recyclePrice =  #{recyclePrice}
	</if>
	<if test="winMoney != null" >
	    and winMoney =  #{winMoney}
	</if>
</trim>
</sql>
   

<!-- 插入记录 -->
<insert id="add" parameterType="Object" >
  insert into order_ticket_cancel(order_id,ticketInfo_id,agent_id,trade_time,trade_price,trade_type,state,recycleState,recyclePrice,winMoney)
 values(#{order_id},#{tid},#{agent_id},#{trade_time},#{trade_price},#{trade_type},#{state},#{recycleState},#{recyclePrice},#{winMoney})
</insert>

<!-- 根据id，修改记录-->  
 <update id="update" parameterType="Object" >
  update order_ticket_cancel set ticketInfo_id=#{tid},agent_id=#{agent_id},trade_time=#{trade_time},trade_price=#{trade_price},trade_type=#{trade_type},state=#{state},recycleState=#{recycleState},recyclePrice=#{recyclePrice},winMoney=#{winMoney} where order_id=#{order_id}
 </update>
 
 <!-- 修改记录，只修改只不为空的字段 -->
<update id="updateBySelective" parameterType="Object" >
	update order_ticket_cancel set 
	<trim  suffixOverrides="," >
	<if test="tid != null   and tid!=''" >
		ticketInfo_id=#{tid},
	</if>
	<if test="agent_id != null  ">
		agent_id=#{agent_id},
	</if>
	<if test="trade_time != null  ">
		trade_time=#{trade_time},
	</if>
	<if test="trade_price != null  ">
		trade_price=#{trade_price},
	</if>
	<if test="trade_type != null  ">
		trade_type=#{trade_type},
	</if>
	<if test="state != null  ">
		state=#{state},
	</if>
	<if test="recycleState != null  ">
		recycleState=#{recycleState},
	</if>
	<if test="recyclePrice != null  ">
		recyclePrice=#{recyclePrice},
	</if>
	<if test="winMoney != null  ">
		winMoney=#{winMoney},
	</if>
	</trim> where order_id=#{order_id}
</update>

<!-- 删除记录 -->
<delete id="delete" parameterType="Object">
	delete 	 from order_ticket_cancel where order_id = #{order_id}
</delete>
 
<!-- 根据id查询 order_ticket_cancel -->
<select id="queryById"  resultMap="BaseResultMap" parameterType="Object">
	select <include refid="Base_Column_List" /> 
	 from order_ticket_cancel where order_id = #{order_id}
</select>

<!-- order_ticket_cancel 列表总数-->
<select id="queryByCount" resultType="java.lang.Integer"  parameterType="Object">
	select count(1) from order_ticket_cancel 
	<include refid="Example_Where_Clause"/>
</select>
  	
<!-- 查询order_ticket_cancel列表 -->
<select id="queryByList" resultMap="BaseResultMap"  parameterType="Object">
	select 
	<include refid="Base_Column_List"/>
	from order_ticket_cancel 
	<include refid="Example_Where_Clause"/>
	<if test="pager.orderCondition != null and pager.orderCondition != ''" >
      ${pager.orderCondition}
    </if>
    <if test="pager.mysqlQueryCondition != null and pager.mysqlQueryCondition != ''" >
       ${pager.mysqlQueryCondition}
    </if>
</select>
  	
</mapper>   
