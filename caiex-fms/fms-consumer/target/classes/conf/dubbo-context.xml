    <dubbo:reference  id= "loginService" interface="com.fms.api.LoginService"/>
    <dubbo:reference  id= "dailyFinancialDetailService" interface="com.fms.api.DailyFinancialDetailService"/>
     <dubbo:reference  id= "dailyCollectStatementsService" interface="com.fms.api.DailyCollectStatementsService"/>
     <dubbo:reference  id= "dailyFinancialStatementsService" interface="com.fms.api.DailyFinancialStatementsService"/>
     <dubbo:reference  id= "detailAllupService" interface="com.fms.api.DetailAllupService"/>
    <dubbo:reference  id= "detailSGLService" interface="com.fms.api.DetailSGLService"/>
    
    <dubbo:reference  id= "hisDailyFinancialDetailService" interface="com.fms.api.HisDailyFinancialDetailService"/>
	<dubbo:reference  id= "hisDailyCollectStatementsService" interface="com.fms.api.HisDailyCollectStatementsService"/>
