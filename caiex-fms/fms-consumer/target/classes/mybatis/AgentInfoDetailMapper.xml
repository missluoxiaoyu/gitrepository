<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.caiex.account.mapper.AgentInfoDetailMapper">
	
	<!--渠道  添加明细detail  -->
	<insert id="addPreStoreDetail" parameterType="com.caiex.account.entity.AgentInfoDetail">
	  insert into agent_info_detail
		(agentCode,agentName,oldPreStore,newPreStore,preStoreDetail,operateTime)
        values(#{agentCode},#{agentName},#{oldPreStore},#{newPreStore},#{preStoreDetail},#{operateTime})
	</insert>
		
	<!--渠道 查询明细  -->
	<select id="queryPreStoreDetail" resultMap="agentInfoDetailResult" parameterType="java.lang.String">
	select * from agent_info_detail where agentCode =#{agentCode} order by operateTime asc
	</select>
	
	
		
		
	<resultMap type="com.caiex.account.entity.AgentInfoDetail" id="agentInfoDetailResult">
 	<id column="id" property="id"/>
 	<result column="agentCode" property="agentCode"/>
 	<result column="agentName" property="agentName"/>
 	<result column="oldPreStore" property="oldPreStore"/>
 	<result column="newPreStore" property="newPreStore"/>
 	<result column="preStoreDetail" property="preStoreDetail"/>
 	<result column="operateTime" property="operateTime"/>
	</resultMap>
 
</mapper>   
